# JWT == JSON Web Tokens. This is how Nextcloud authenticates itself to OnlyOffice.
JWT_ENABLED=true
JWT_SECRET={{ onlyoffice_secret | mandatory }}

# Setting this to false will make the documentserver's entrypoint
# script allow unauthorized HTTPS connections. Use only for testing.
JWT_REJECT_UNAUTHORIZED={% if onlyoffice_reject_unauthorized %}true{% else %}false{% endif %}
