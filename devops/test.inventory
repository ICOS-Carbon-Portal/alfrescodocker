# This is an ansible inventory file used for testing. It uses the hostnames
# 'ubuntu', 'centos' and 'bionic' which are VMs created by Vagrant (look in
# Vagrantfile).

[all:vars]
linux_timezone_path=/usr/share/zoneinfo/Europe/Stockholm
linux_add_ssh_root=
certbot_install_certificate=False
check_infrastructure_version=False

[icosprod]
ubuntu
centos
bionic

[icosprod2]
bionic
ubuntu
centos

[stiltmaster]
bionic

[stiltcluster]
ubuntu stiltcluster_port=2561 stiltcluster_ip=192.168.99.20
centos stiltcluster_port=2562 stiltcluster_ip=192.168.99.10
