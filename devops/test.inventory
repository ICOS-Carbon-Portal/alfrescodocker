# This is an ansible inventory file used for testing. It uses the hostnames
# 'centos' and 'bionic' which are VMs created by Vagrant (look in Vagrantfile).

[all:vars]
linux_timezone_path=/usr/share/zoneinfo/Europe/Stockholm
linux_add_ssh_root=
certbot_install_certificate=False
check_infrastructure_version=False
cpmeta_filestorage_target=/disk/data/metaAppStorage
docker_compose_src_parent=/disk/data


[icosprod]
centos

[icosprod:vars]

[icosprod2]
bionic

[icosprod2:vars]


[stiltmaster]
bionic

[stiltmaster:vars]
stiltcluster_port=2561
stiltcluster_maxcores=0

[stiltcluster]
centos stiltcluster_ip=192.168.99.10 stiltcluster_port=2562

