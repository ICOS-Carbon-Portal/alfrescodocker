# This is an ansible inventory file used for testing. It uses the hostnames
# 'ubuntu' and 'centos', which are VMs created by Vagrant (look in Vagrantfile).

[icosprod]
ubuntu
centos
bionic

[icosprod:vars]
linux_timezone_path=/usr/share/zoneinfo/Europe/Stockholm
linux_add_ssh_root=
get_ssl=False
certbot_install_certificate=False

[stiltmaster]
bionic

[stiltcluster]
ubuntu stiltcluster_port=2561
centos stiltcluster_port=2562
bionic stiltcluster_port=2563
